/*******************************
         Site Overrides
*******************************/


.ui.steps .step[data-status=running] {
  color: @teal;
}

.ui.steps .step[data-status=success] {
  color: @green;
}

.ui.steps .step[data-status=failure] {
  color: @red;
}

.ui.steps .step[data-status=unknown] {
  color: @yellow;
}

.ui.steps .step[data-status=waiting] {
  color: @blue;

}